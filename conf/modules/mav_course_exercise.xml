<!DOCTYPE module SYSTEM "module.dtd">

<module name="mav_course_exercise">
<doc>
  <description>
    Exercise module for Autonomous Flight of MAV Course
  </description>
</doc>
  <settings>
    <dl_settings>
      <dl_settings name="Navigation_module_parameters">
        <dl_setting var="count_avoidance" min="0" step="1" max="15" shortname="count_avoidance"/>
        <dl_setting var="Max_heading_rate" min="0" step=".5" max="25" shortname="Max_heading_rate_deg_s"/>
      </dl_settings>
    </dl_settings>
  </settings>
<depends>cv_detect_color_object</depends>
<header>
  <file name="mav_exercise.h"/>
</header>
<init fun="mav_exercise_init()"/>
<periodic fun="mav_exercise_periodic()" freq="4"/>
<makefile target="ap|nps">
  <file name="mav_exercise.c"/>
</makefile>
</module>